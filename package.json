{
  "name": "franklin-chat-service",
  "version": "1.1.20",
  "scripts": {
    "build": "tsc",
    "dev": "dotenv -c $NODE_ENV -- ts-node-dev src/app.ts",
    "start": "dotenv -c $NODE_ENV -- npm run build && dotenv -c $NODE_ENV -- node dist/app.js",
    "build:docker": "npm run build && dotenv -- cross-var \"scripts/build-docker-image.sh %npm_package_version%\"",
    "deploy": "dotenv -- cross-var \"scripts/deploy-to-k8s.sh %npm_package_version%\"",
    "build:release": "npm run build:docker && npm run deploy",
    "set:context": "dotenv -- cross-var \"scripts/set-k8s-context.sh\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@magic-sdk/admin": "^1.9.2",
    "@slack/bolt": "3.13.0",
    "cors": "^2.8.5",
    "dotenv": "16.0.3",
    "express": "^4.18.2",
    "googleapis": "^118.0.0",
    "prom-client": "^14.2.0",
    "socket.io": "^4.6.1",
    "winston": "^3.8.2",
    "winston-loki": "^6.0.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "cross-var": "1.1.0",
    "dotenv-cli": "7.1.0",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.5"
  }
}
